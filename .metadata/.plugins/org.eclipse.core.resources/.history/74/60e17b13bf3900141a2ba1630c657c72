package by.pvt.epam.entity;

public class Employee {
	private String name;
	private String surname;
	private String login;
	private String pass;
	private Position position;
	private Role role;

	public Employee() {
	}

	public Employee(String name, String surname, String login, String pass,
			Position position, Role role) {
		this.name = name;
		this.surname = surname;
		this.login = login;
		this.pass = pass;
		this.position = position;
		this.role = role;
	}

}
