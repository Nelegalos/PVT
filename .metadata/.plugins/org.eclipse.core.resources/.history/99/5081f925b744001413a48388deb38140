package by.pvt.epam.command;

import javax.servlet.http.HttpServletRequest;
import by.pvt.epam.resource.ConfigurationManager;

public class RegisterEmployeeCommand implements ActionCommand {

	@Override
	public String execute(HttpServletRequest request) {

		request.setAttribute("employeeAdded", "employee.added");
		request.removeAttribute("employeeNotAdded");
		String page = ConfigurationManager.getProperty("path.page.staff");
		return page;
	}

}
